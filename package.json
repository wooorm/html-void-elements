{
  "name": "html-void-elements",
  "version": "1.0.1",
  "description": "List of known void HTML elements",
  "license": "MIT",
  "keywords": [
    "html",
    "void",
    "tag",
    "name",
    "element",
    "tagname",
    "w3c",
    "whatwg"
  ],
  "repository": "https://github.com/wooorm/html-void-elements",
  "bugs": "https://github.com/wooorm/html-void-elements/issues",
  "author": "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)",
  "contributors": [
    "Titus Wormer <tituswormer@gmail.com> (http://wooorm.com)"
  ],
  "main": "index.js",
  "files": [
    "index.json"
  ],
  "dependencies": {},
  "devDependencies": {
    "bail": "^1.0.0",
    "browserify": "^14.1.0",
    "esmangle": "^1.0.0",
    "jsdom": "^9.8.3",
    "remark-cli": "^2.1.0",
    "remark-preset-wooorm": "^1.0.0",
    "tape": "^4.0.0",
    "xo": "^0.17.0"
  },
  "scripts": {
    "build-md": "remark . --quiet --frail --output",
    "build-crawl": "node build",
    "build-bundle": "browserify index.json --bare -s htmlVoidElements > html-void-elements.js",
    "build-mangle": "esmangle html-void-elements.js > html-void-elements.min.js",
    "build": "npm run build-md && npm run build-bundle && npm run build-mangle",
    "lint": "xo",
    "test-api": "node test.js",
    "test": "npm run build && npm run lint && npm run test-api"
  },
  "xo": {
    "space": true,
    "ignores": [
      "html-void-elements.min.js"
    ]
  },
  "remarkConfig": {
    "presets": "wooorm"
  }
}
